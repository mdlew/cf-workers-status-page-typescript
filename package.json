    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-polish-tagged-templates": "^0.2.1",
    "vite": "^6.3.5"
