{
  "name": "cf-worker-status-page-typescript",
  "type": "module",
  "scripts": {
    "dev": "tsx ./dev-server",
    "public:assets": "tsx ./scripts/public-assets.ts",
    "build": "pnpm run wrangler:types && tsc && pnpm run public:assets && vike build",
    "preview": "pnpm run build && wrangler dev",
    "preview:production": "pnpm run build && wrangler dev --env production",
    "deploy": "pnpm run build && wrangler deploy --env production",
    "prepare": "husky",
    "wrangler:types": "wrangler types",
    "postinstall": "pnpm run public:assets"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4",
    "@floating-ui/react": "^0.27.16",
    "@vitejs/plugin-react": "^5.0.0",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-streaming": "^0.3.50",
    "tagged-classnames-free": "^0.1.1",
    "vike": "^0.4.237",
    "wrangler": "latest"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250819.0",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@types/node-fetch": "^2.6.13",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "dedent": "^1.6.0",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-plugin-iconify": "^2.0.0",
    "ts-node": "^10.9.2",
    "tsx": "latest",
    "type-fest": "^4.41.0",
    "typescript": "latest",
    "unplugin-auto-import": "^20.0.0",
    "vite": "^7.1.2"
  }
}
