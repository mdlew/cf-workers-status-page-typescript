    "node-fetch": "^2.7.0",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-streaming": "^0.3.50",
    "tagged-classnames-free": "^0.1.1",
    "vike": "^0.4.231",
    "wrangler": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@cloudflare/workers-types": "^4.20250607.0",
    "@commitlint/cli": "^19.8.1",
