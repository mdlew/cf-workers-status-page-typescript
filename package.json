{
  "name": "cf-worker-status-page-typescript",
  "type": "module",
  "scripts": {
    "dev": "tsx ./dev-server",
    "public:assets": "tsx ./scripts/public-assets.ts",
    "build": "pnpm run wrangler:types && tsc && pnpm run public:assets && vike build",
    "preview": "pnpm run build && wrangler dev",
    "preview:production": "pnpm run build && wrangler dev --env production",
    "deploy": "pnpm run build && wrangler deploy --env production",
    "prepare": "husky",
    "wrangler:types": "wrangler types",
    "postinstall": "pnpm run public:assets"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.1",
    "@floating-ui/react": "^0.27.16",
    "clsx": "^2.1.1",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-streaming": "^0.4.15",
    "vike": "^0.4.250"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260103.0",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "dedent": "^1.7.1",
    "express": "^5.2.1",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-plugin-iconify": "^2.0.0",
    "tsx": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "^20.3.0",
    "vite": "^7.3.0",
    "wrangler": "latest"
  }
}
