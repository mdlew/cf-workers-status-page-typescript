    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "compression": "^1.8.0",
    "dedent": "^1.6.0",
    "eslint": "^9.9.1",
    "eslint-plugin-format": "^0.1.3",
