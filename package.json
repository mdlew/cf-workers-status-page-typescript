{
  "name": "cf-worker-status-page-typescript",
  "type": "module",
  "scripts": {
    "dev": "tsx ./dev-server",
    "public:assets": "tsx ./scripts/public-assets.ts",
    "build": "pnpm run wrangler:types && tsc && pnpm run public:assets && vike build",
    "preview": "pnpm run build && wrangler dev",
    "preview:production": "pnpm run build && wrangler dev --env production",
    "deploy": "pnpm run build && wrangler deploy --env production",
    "prepare": "husky",
    "wrangler:types": "wrangler types",
    "postinstall": "pnpm run public:assets"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.2",
    "@floating-ui/react": "^0.27.17",
    "clsx": "^2.1.1",
    "papaparse": "^5.5.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.3",
    "react-streaming": "^0.4.15",
    "vike": "^0.4.253"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "dedent": "^1.7.1",
    "express": "^5.2.1",
    "husky": "^9.1.7",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "latest",
    "typescript": "latest",
    "vite": "^7.3.1",
    "wrangler": "latest"
  }
}
